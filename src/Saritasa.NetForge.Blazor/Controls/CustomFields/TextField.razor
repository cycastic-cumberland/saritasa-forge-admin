@inherits CustomField

<MudTextField T="string"
              Label="@Property.Name"
              Variant="Variant.Outlined"
              InputType="InputType.Text"
              HelperText="@Property.Description"
              Disabled="IsReadOnly"
              @bind-Value="PropertyValue"
              Lines="Lines"
              MaxLines="MaxLines"
              AutoGrow="IsAutoGrow">
</MudTextField>


